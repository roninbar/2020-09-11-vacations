{
  "name": "Vacations",
  "description": "An app that lets users follow vacations",
  "keywords": [],
  "stack": "heroku-20",
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    }
  ],
  "env": {
    "JAWSDB_MARIA_URL": {
      "description": "URL of MariaDB server",
      "value": "mysql://<username>:<password>@<hostname>:3306/<database>"
    },
    "DEBUG": {
      "description": "Mask for log messages",
      "value": "server:*",
      "required": false
    },
    "SECRET": {
      "description": "A secret key for signing passwords and session cookies",
      "generator": "secret"
    },
    "SIDNAME": {
      "description": "Session ID cookie name",
      "value": "connect.sid",
      "required": false
    }
  }
}